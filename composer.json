{
    "name": "icernn03/msgowl-laravel",
    "description": "Msgowl API SDK for Laravel",
    "keywords": [
        "icernn03",
        "msgowl",
        "laravel"
    ],
    "homepage": "https://github.com/icernn03/msgowl-laravel",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "icernn03",
            "email": "icernn03@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "guzzlehttp/guzzle": "^7.9",
        "illuminate/support": "11.43.*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.11",
        "phpunit/phpunit": "^10.5.0"
    },
    "autoload": {
        "psr-4": {
            "Icernn03\\Msgowl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icernn03\\Msgowl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Icernn03\\Msgowl\\MsgowlServiceProvider"
            ],
            "aliases": {
                "Msgowl": "Icernn03\\Msgowl\\MsgowlFacade"
            }
        }
    }
}
